{
  "name": "shiro-guessr",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build && wrangler pages dev",
    "build": "ng build && npm run process",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:shiro-guessr": "node dist/shiro-guessr/server/server.mjs",
    "process": "node ./tools/copy-files.mjs",
    "deploy": "npm run build && npx wrangler pages deploy",
    "cf-typegen": "wrangler types"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.0",
    "@types/node": "^20.17.19",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "xhr2": "^0.2.1"
  }
}
